@model MercDevs_ej2.Models.Diagnosticosolucion

@{
    ViewData["Title"] = "Crear Diagnóstico-Solución";
}

<h1>Crear Diagnóstico-Solución</h1>

<form asp-action="PostCrearPorDatos" method="post">
    <div class="form-group">
        <label asp-for="DescripcionDiagnostico" class="control-label"></label>
        <input asp-for="DescripcionDiagnostico" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="DescripcionSolucion" class="control-label"></label>
        <input asp-for="DescripcionSolucion" class="form-control" />
    </div>
    <input type="hidden" asp-for="DatosFichaTecnicaId" value="@ViewBag.DatosFichaTecnicaId" />
    <button type="submit" class="btn btn-primary">Guardar</button>
</form>
